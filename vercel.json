{
  "version": 2,
  "builds": [
    {
      "src": "api/ai-response.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/ai-response",
      "dest": "/api/ai-response.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
